	border-image: url(playlist_toggle.png);
	qproperty-icon: url(playlist_icon.png);
	border-top: 8;
	border-left: 10;
	border-right: 10;
	padding: -8;
	padding-top: -14;
}

#playlistToggleButton:hover {
	border-image: url(playlist_toggle_hover.png);
}

#playlistToggleButton:pressed {
	border-image: url(playlist_toggle_pressed.png);
}

#mainWindow &gt; QSizeGrip, #sizeGrip {
	background-image: url(size_grip.png);
	background-repeat: none;
}

#iconLabel {
	border-image: url(icon.png);
}

#buttonSplitter_1, #buttonSplitter_2, #buttonSplitter_3 {
	border-image: url(button_splitter.png);
	border-top: 1;
}

#prevButton, #playButton, #stopButton, #nextButton {
	border-image: url(button_normal.png);
	min-height:15;
	max-height:15;
	border: 5;
}

#prevButton, #playButton, #stopButton {
	margin-right: -5;
	padding-right: 5;
}

#playButton, #stopButton,  #nextButton {
	margin-left: -5;
	padding-left: 5;
}

#playButton:hover, #stopButton:hover,
#nextButton:hover, #prevButton:hover {
	border-image: url(button_hover.png);
}

#playButton:pressed, #stopButton:pressed,
#nextButton:pressed, #prevButton:pressed {
	border-image: url(button_pressed.png);
}


#volumeSlider::groove:horizontal {
	border-image: url(slider_groove.png);
	border-width: 5;
	height: 1;
}

#volumeSlider::handle:horizontal {
	border-image: url(slider_handle_small.png);
	border-width: 4;
	width: 18;
	margin: -6 -6 -4 -6;
}

#minimizeButton, #closeButton {
	border-image: none;
	background-image: url(title_button_normal.png);
