	border-bottom: 50;
}

#shadowWidget {
	border-image: url(playlist_shadow.png);
}

#playlistWidget {
	background: white;
	padding: 5;
}

#playlistWidget::item {
	background: white;
}

#playlistWidget::item:alternate {
	background: palette(alternate-base);
}

#playlistWidget::item:selected {
	background: #BBBBBB /*palette(highlight)*/;
}

